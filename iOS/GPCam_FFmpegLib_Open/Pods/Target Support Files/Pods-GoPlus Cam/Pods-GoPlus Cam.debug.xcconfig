CLANG_CXX_LANGUAGE_STANDARD = c++11
CLANG_CXX_LIBRARY = libstdc++
FRAMEWORK_SEARCH_PATHS = $(inherited) "${PODS_ROOT}/PLCrashReporter/iOS Framework"
GCC_PREPROCESSOR_DEFINITIONS = $(inherited) COCOAPODS=1
HEADER_SEARCH_PATHS = $(inherited) "${PODS_ROOT}/Headers/Public" "${PODS_ROOT}/Headers/Public/PLCrashReporter"
OTHER_CFLAGS = $(inherited) -isystem "${PODS_ROOT}/Headers/Public" -isystem "${PODS_ROOT}/Headers/Public/PLCrashReporter"
OTHER_LDFLAGS = $(inherited) -ObjC -l"bz2" -l"iconv" -l"stdc++" -l"xml2" -l"z" -framework "AVFoundation" -framework "AudioToolbox" -framework "CFNetwork" -framework "CoreGraphics" -framework "CoreText" -framework "CrashReporter" -framework "OpenGLES" -framework "QuartzCore" -framework "Security"
PODS_ROOT = ${SRCROOT}/Pods
